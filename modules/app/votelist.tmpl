<div class="vote_header">
	<h2 class="ilb vote_title">给最感人的视频点个赞吧</h2>
	<p class="ilb leftvotenum">今日可投票数:<span><%=leftvotenum%></span></p>
</div>
<div class="vote_list">
	<ul>
		<%for (var i=0;i<data.length;i++){%>
			<li class="vote_item">
				<a href="<%=data[i].imgh_url%>" class="vote_poster" target="_blank">
					<img src="<%=data[i].imgh_url%>">
				</a>
				<a href="<%=data[i].imgh_url%>" class="vote_post_title"><%=data[i].title%></a>
				<div class="wrapvote">
					<a href="javascript:void(0)" class="tovote fl <%if(data[i].uid_has_voted){%>voted<%}%>" workid = "<%=data[i].works_id%>">
						<%if(data[i].uid_has_voted){%>
							已赞
						<%} else {%>
							赞一下
						<%}%>

					</a>
					<span class="votenum fl"><%=data[i].vote_num%>票</span>
				</div>
			</li>
		<%}%>
	</ul>
</div>